{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [
            "> 0.5%",
            "not ie 11",
            "not edge 17"
          ]
        }
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-class-properties"
    ],
    [
      "@babel/plugin-transform-runtime",
      {
        "regenerator": true
      }
    ]
  ]
}
